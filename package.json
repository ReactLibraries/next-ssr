{
  "name": "next-ssr",
  "version": "1.0.0",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js"
  },
  "description": "Library for easy SSR of data obtained from asynchronous in Next.js.",
  "license": "MIT",
  "scripts": {
    "build": "tsc && tsc --project ./tsconfig.esm.json && cpy esm dist",
    "test": "jest --coverage",
    "test:update": "jest -u --coverage",
    "lint": "eslint --fix ./src ./test",
    "lint:fix": "eslint --fix ./src ./test"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@swc-node/jest": "^1.8.3",
    "@swc/core": "^1.6.6",
    "@swc/helpers": "^0.5.11",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "cpy-cli": "^5.0.0",
    "eslint": "9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.5.2",
    "typescript-eslint": "^7.14.1"
  },
  "keywords": [
    "nextjs",
    "react",
    "ssr",
    "fetch"
  ],
  "repository": "https://github.com/ReactLibraries/next-ssr",
  "author": "SoraKumo <info@croud.jp>"
}
